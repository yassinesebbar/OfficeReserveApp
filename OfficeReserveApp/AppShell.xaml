<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="OfficeReserveApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:OfficeReserveApp.MVVM.Views"
    Shell.FlyoutBehavior="Flyout"
    x:Name="Shell">


    <FlyoutItem Route="Login"
                x:Name="LoginPage">
        <ShellContent ContentTemplate="{DataTemplate views:LoginPage}" Route="LoginPage"/>
    </FlyoutItem>

    <FlyoutItem Route="OfficeManagementOverview"
                x:Name="OfficeManagementOverviewPage">
        <ShellContent ContentTemplate="{DataTemplate views:OfficeManagementOverviewPage}" Route="OfficeManagementOverviewPage"/>
    </FlyoutItem>

    <ShellContent
        x:Name="DashboardPage"
        Title="Home"
        ContentTemplate="{DataTemplate views:DashboardPage}"
        Route="DashboardPage" />

    <ShellContent
        x:Name="WorkSpotOverviewPage"
        Title="WorkSpotOverviewPage"
        ContentTemplate="{DataTemplate views:WorkSpotOverviewPage}"
        Route="WorkSpotOverviewPage" />



</Shell>
